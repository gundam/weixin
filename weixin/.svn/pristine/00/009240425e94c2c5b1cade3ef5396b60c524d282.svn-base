			<style type="text/css">

			#adminStore_add_div{

				width: 400px;
				
				margin: 0 auto 0 auto;


			}
			</style>

			<script type="text/javascript">
				jQuery(document).ready(function($) {
					//获取商店名称
					$.get();
				});
				

			</script>

